modules = ["python-3.11"]
[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Run AdminToolbox"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Run AdminToolbox GUI"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Run AdminToolbox Tests"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Debug AdminToolbox"

[[workflows.workflow]]
name = "Run AdminToolbox"
author = "agent"

[workflows.workflow.metadata]
agentRequireRestartOnSave = false

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python admin_toolbox.py"
waitForPort = 5000

[[workflows.workflow]]
name = "Run AdminToolbox GUI"
author = "agent"

[workflows.workflow.metadata]
agentRequireRestartOnSave = false

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python admin_toolbox.py"

[[workflows.workflow]]
name = "Run AdminToolbox Tests"
author = "agent"

[workflows.workflow.metadata]
agentRequireRestartOnSave = false

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python -m pytest tests/test_admin_toolbox.py"

[[workflows.workflow]]
name = "Debug AdminToolbox"
author = "agent"

[workflows.workflow.metadata]
agentRequireRestartOnSave = false

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python admin_toolbox.py"

[nix]
channel = "stable-24_05"

[deployment]
run = ["sh", "-c", "python admin_toolbox.py"]

